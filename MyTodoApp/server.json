{
    "name":"ColdFusionMVC",
    "app":{
        "cfengine":"adobe@2023"
    },
    "web":{
        "directoryBrowsing":true,
        "webAdminEnabled":true,
        "webAdminPassword":"${adminPassword}",
        "allowCFAdmin":true,
        "host":"${HOST}",
        "http":{
            "port":"${PORT}"
        },
        "SSL":{
            "enable":"${SSL}",
            "port":"${SSLPORT}"
        }
    },
    "jvm":{
        "heapSize":512
    }
}
